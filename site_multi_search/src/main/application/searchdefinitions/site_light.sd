search site {



document site_light {

        field text type string {
            indexing: summary | index
        }

        field title type string {
            indexing: summary | index
        }

    }

    fieldset default {
        fields: title, text
    }

    rank-profile post inherits default {

        first-phase {
            expression:nativeRank(title, text)
        }

    }

}
